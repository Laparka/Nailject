{
  "name": "nailject",
  "version": "0.0.1",
  "description": "The TypeScript-only Dependency Injection Library based on generated Service Locators for each registered service",
  "scripts": {
    "test": "jest --watch",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint",
    "start": "node bin/nailject.js"
  },
  "main": "index.js",
  "bin": {
    "nailject": "bin/nailject.js"
  },
  "keywords": [
    "dependency injection",
    "ioc",
    "service locator"
  ],
  "author": "Marat Sultangareev",
  "license": "ISC",
  "engines": {
    "node": "^10.12.0 || >=12.0.0"
  },
  "dependencies": {
    "typescript": "^4.0.5",
    "@types/node": "^14.14.8",
    "commander": "^6.2.1",
    "fs": "0.0.1-security"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.13.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3"
  }
}
