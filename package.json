{
  "name": "nailject",
  "version": "0.0.1",
  "description": "The TypeScript-only Dependency Injection Library based on generated Service Locators for each registered service",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest --watch",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "start": "tsc & node lib/index.js"
  },
  "keywords": ["Dependency Injection", "IoC", "ServiceLocator"],
  "author": "Marat Sultangareev",
  "license": "ISC",
  "bin": {
    "ioc": "index.ts"
  },
  "dependencies": {
    "@types/node": "^14.14.8",
    "@typescript-eslint/typescript-estree": "^4.9.0",
    "fs": "0.0.1-security"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "eslint": "^7.13.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  }
}
