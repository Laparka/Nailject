{
  "name": "pileuple",
  "version": "0.0.8",
  "description": "Compile-time IoC and Dependency Injection Framework library for TypeScript.",
  "homepage": "https://github.com/Laparka/pileuple",
  "repository": {
    "type": "git",
    "url": "https://github.com/Laparka/pileuple.git"
  },
  "bugs": {
    "url": "https://github.com/Laparka/pileuple/issues"
  },
  "scripts": {
    "test": "jest --watch",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint",
    "start": "tsc & node bin/pileuple.js ./__tests__/module.ts --moduleName Module --outputDir ./__tests__/.generated/"
  },
  "main": "index.js",
  "bin": {
    "pileuple": "bin/pileuple.js"
  },
  "type": "commonjs",
  "keywords": [
    "dependency injection",
    "ioc",
    "compile-time IoC"
  ],
  "author": " <laparka.shops@gmail.com>",
  "license": "ISC",
  "engines": {
    "node": "^10.12.0 || >=12.0.0"
  },
  "dependencies": {
    "commander": "^6.2.1",
    "fs": "0.0.1-security",
    "liquidjs": "^9.19.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.8",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.13.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.5"
  }
}
